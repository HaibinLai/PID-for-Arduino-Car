123 {\cf17 inline} {\cf18 void} Get_sensor_status()\{\par
124     rr = digitalRead(Right_SensorB);  {\cf20 // Left outside}\par
125     rm = digitalRead(Right_SensorA);  {\cf20 // Left inside}\par
126     md = digitalRead(Middle_Sensor);  {\cf20 // Middle}\par
127     lm = digitalRead(Left_SensorA);  {\cf20 // Right inside}\par
128     ll = digitalRead(Left_SensorB);  {\cf20 // Right outside}\par
129 \par
130     counter += 1;\par
131 \par
132     {\cf20 // 0 0 1 0 0 }\par
133     {\cf19 if}(rr == 0 && rm == 0 && md == 1 && lm == 0 && ll == 0 )\{\par
134       current_status = 0;\par
135       I = 0;\par
136       go_stright_value += 1;\par
137       {\cf20 // I_angular = 0;}\par
138       wdt_reset();\par
139     \}\par
140     {\cf19 else} {\cf19 if}(rr == 0 && rm == 1 && md == 1 && lm == 0 && ll == 0)\{\par
141       current_status = -1;\par
142       go_stright_value = 0;\par
143       wdt_reset();\par
144     \}\par
145     {\cf19 else} {\cf19 if}(rr == 0 && rm == 1 && md == 0 && lm == 0 && ll == 0)\{\par
146       current_status = -3;\par
147       go_stright_value = 0;\par
148       wdt_reset();\par
149     \}\par
150     {\cf19 else} {\cf19 if}(rr == 1 && rm == 1 && md == 0 && lm == 0 && ll == 0)\{\par
151       current_status = -5;\par
152       go_stright_value = 0;\par
153       wdt_reset();\par
154     \}\par
155     {\cf19 else} {\cf19 if}(rr == 1 && rm == 0 && md == 0 && lm == 0 && ll == 0)\{\par
156       current_status = -9;\par
157       last_dir = -20;\par
158       go_stright_value = 0;\par
159       wdt_reset();\par
160     \}\par
161 \par
162     {\cf19 else} {\cf19 if}(rr == 0 && rm == 0 && md == 1 && lm == 1 && ll == 0)\{\par
163       current_status = 1;\par
164       go_stright_value = 0;\par
165       wdt_reset();\par
166     \}\par
167     {\cf19 else} {\cf19 if}(rr == 0 && rm == 0 && md == 0 && lm == 1 && ll == 0)\{\par
168       current_status = 3;\par
169       go_stright_value = 0;\par
170       wdt_reset();\par
171     \}\par
172     {\cf19 else} {\cf19 if}(rr == 0 && rm == 0 && md == 0 && lm == 1 && ll == 1)\{\par
173       current_status = 5;\par
174       go_stright_value = 0;\par
175       wdt_reset();\par
