209 {\cf17 inline} {\cf18 void} Update_Speed()\{\par
210     \par
211     Left_wheel_speed = cursor_speed + PID_value;\par
212     Right_wheel_speed = cursor_speed - PID_value;\par
213 \par
214     {\cf19 if}(go_stright_value >= 500)\{\par
215       Left_wheel_speed = HIGH_SPEED_LOW_DRAG;\par
216       Right_wheel_speed = HIGH_SPEED_LOW_DRAG;\par
217     \}\par
218 \par
219     {\cf19 if}(Left_wheel_speed > LOW_SPEED)\{\par
220         Left_wheel_speed = LOW_SPEED;\par
221     \}\par
222     {\cf19 if}(Left_wheel_speed < max_speed)\{\par
223         Left_wheel_speed = max_speed;\par
224     \}\par
225 \par
226     {\cf19 if}(Right_wheel_speed > LOW_SPEED)\{\par
227         Right_wheel_speed = LOW_SPEED;\par
228     \}\par
229     {\cf19 if}(Right_wheel_speed < max_speed)\{\par
230         Right_wheel_speed = max_speed;\par
231     \}\par
232 \par
233     digitalWrite(4,HIGH);\par
234     analogWrite(5, Right_wheel_speed);\par
235     digitalWrite(7,HIGH);\par
236     analogWrite(6, Left_wheel_speed);\par
237 \}\par
