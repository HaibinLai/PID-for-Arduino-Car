195 {\cf17 inline} {\cf18 void} calculate_pid()\{\par
196   P = current_status;\par
197   I = I + current_status;\par
198   {\cf20 // I_angular = I_angular + current_status * 0.001;}\par
199   D = current_status - previous_status;\par
200  \par
201   PID_value = (Kp * P) + (Ki * I) + (Kd * D);\par
202  \par
203   previous_status = current_status;\par
204 \}\par
